{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Alcohol drinking status of daily drinking(%) for people aged 14 and over between 2022-2023",
  "width": 800,
  "height": 450,
  "projection": {"type": "mercator"},
  "data": {
    "url": "ne_10m_admin_1_states_provinces.topojson", 
    "format": {"type": "topojson", "feature": "ne_10m_admin_1_states_provinces"}
  },
  "transform": [
    {
      "lookup": "properties.name",
      "from": {
        "data": {
          "url": "alcohol_drinking_status.csv"
        },
        "key": "State",
        "fields": ["Daily"]
      }
    }
  ],
  "mark": {"type": "geoshape"},
  "encoding": {
    "color": {
      "field": "Daily",
      "type": "quantitative"
    },
    "tooltip": [
      {"field": "properties.name", "type": "nominal", "title": "State"},
      {"field": "Daily", "type": "quantitative"}
    ]
  }
}